{
   "name": "winery",
   "version": "1.0.0",
   "main": "index.js",
   "repository": "https://github.com/PhMoraiis/Winery.git",
   "author": "Philipe Barbosa de Morais <liperapltda@gmail.com>",
   "license": "MIT",
   "scripts": {
      "dev": "tsnd --respawn --transpile-only --exit-child --ignore-watch node_modules app/index.ts",
      "build": "tsc",
      "start": "ts-node app/index.ts",
      "typeorm": "typeorm-ts-node-commonjs",
      "typeorm:run": "npm run typeorm migration:run -d app/data-source.ts",
      "typeorm:revert": "npm run typeorm migration:revert -d app/data-source.ts",
      "typeorm:create": "npm run typeorm migration:create app/migrations/",
      "typeorm:generate": "typeorm migration:generate -d app/data-source.ts app/migrations/default"
   },
   "dependencies": {
      "dotenv": "^16.0.3",
      "express": "^4.18.2",
      "pg": "^8.4.0",
      "reflect-metadata": "^0.1.13",
      "typeorm": "0.3.15",
      "uuid": "^9.0.0"
   },
   "devDependencies": {
      "@types/express": "^4.17.17",
      "@types/node": "^16.11.10",
      "@types/uuid": "^9.0.1",
      "ts-node": "10.7.0",
      "ts-node-dev": "^2.0.0",
      "typescript": "4.5.2"
   }
}
