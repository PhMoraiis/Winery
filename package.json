{
   "name": "winery",
   "version": "1.0.0",
   "main": "index.js",
   "repository": "https://github.com/PhMoraiis/Winery.git",
   "author": "Philipe Barbosa de Morais <liperapltda@gmail.com>",
   "license": "MIT",
   "scripts": {
      "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
      "typeorm": "typeorm-ts-node-commonjs",
      "typeorm:run": "yarn typeorm migration:run -d src/data-source.ts",
      "typeorm:revert": "yarn typeorm migration:revert -d src/data-source.ts",
      "typeorm:create": "yarn typeorm migration:create src/migrations/",
      "start": "ts-node src/index.ts"
   },
   "dependencies": {
      "express": "^4.18.2",
      "pg": "^8.4.0",
      "reflect-metadata": "^0.1.13",
      "typeorm": "0.3.15",
      "uuid": "^9.0.0"
   },
   "devDependencies": {
      "@types/express": "^4.17.17",
      "@types/node": "^16.11.10",
      "@types/uuid": "^9.0.1",
      "ts-node": "10.7.0",
      "ts-node-dev": "^2.0.0",
      "typescript": "4.5.2"
   }
}
