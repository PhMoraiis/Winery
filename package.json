{
   "name": "winery",
   "version": "1.0.0",
   "main": "index.js",
   "repository": "https://github.com/PhMoraiis/Winery.git",
   "author": "Philipe Barbosa de Morais <liperapltda@gmail.com>",
   "license": "MIT",
   "scripts": {
      "dev": "tsnd --respawn --transpile-only --exit-child --ignore-watch node_modules app/index.ts",
      "build": "tsc",
      "start": "ts-node app/index.ts",
      "typeorm": "typeorm-ts-node-commonjs",
      "migration:run": "typeorm-ts-node-commonjs -d ./app/data-source.ts migration:run",
      "migration:generate": "typeorm-ts-node-commonjs -d ./app/data-source.ts migration:generate ./app/migrations/default",
      "typeorm:create": "npm run typeorm migration:create app/migrations/default",
      "typeorm:revert": "npm run typeorm migration:revert -d app/data-source.ts"
   },
   "dependencies": {
      "bcrypt": "^5.1.0",
      "dotenv": "^16.0.3",
      "express": "^4.18.2",
      "jsonwebtoken": "^9.0.0",
      "pg": "^8.4.0",
      "reflect-metadata": "^0.1.13",
      "typeorm": "0.3.15"
   },
   "devDependencies": {
      "@types/bcrypt": "^5.0.0",
      "@types/express": "^4.17.17",
      "@types/jsonwebtoken": "^9.0.2",
      "@types/node": "^16.11.10",
      "nodemon": "^3.0.1",
      "ts-node": "10.7.0",
      "ts-node-dev": "^2.0.0",
      "typescript": "4.5.2"
   }
}
